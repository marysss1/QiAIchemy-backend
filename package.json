{
  "name": "qiaichemy-backend",
  "version": "1.0.0",
  "main": "dist/server.js",
  "scripts": {
    "test": "echo \"No tests yet\"",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/server.js",
    "typecheck": "tsc --noEmit",
    "rag:ingest": "ts-node src/scripts/ragIngest.ts",
    "rag:eval:gen": "ts-node src/scripts/generateEvalSet.ts",
    "rag:eval": "ts-node src/scripts/runRagEval.ts",
    "rag:eval:summary": "ts-node src/scripts/summarizeRagEval.ts",
    "knowledge:build": "ts-node src/scripts/buildKnowledgeCorpus.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "Standalone backend for QiAIchemy (TypeScript + MongoDB auth)",
  "dependencies": {
    "bcryptjs": "^3.0.3",
    "cors": "^2.8.6",
    "dotenv": "^17.3.1",
    "express": "^5.2.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.3",
    "mongoose": "^8.19.1",
    "morgan": "^1.10.1",
    "openai": "^5.21.0",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/node": "^25.3.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3"
  }
}
